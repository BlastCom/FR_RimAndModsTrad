<Defs>

	<RecipeDef Name="SurgeryNatural" ParentName="SurgeryInstallBodyPartNaturalBase" Abstract="True">
		<surgerySuccessChanceFactor>0.8</surgerySuccessChanceFactor>
		<deathOnFailedSurgeryChance>0.3</deathOnFailedSurgeryChance>
		<!--<dontShowIfAnyIngredientMissing>true</dontShowIfAnyIngredientMissing>-->
		<ingredients>
		  <li>
			<filter><categories><li>Medicine</li></categories></filter>
			<count>3</count>
		  </li>
		</ingredients>
	</RecipeDef>

	<!-- EPOE -->

	<RecipeDef ParentName="SurgeryNatural">
		<defName>InstallNaturalEye</defName>
		<label>install eye</label>
		<description>Installs eye.</description>
		<workerClass>Recipe_InstallNaturalBodyPart</workerClass>
		<jobString>Installing eye.</jobString>
		<workAmount>1200</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Eye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Eye</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
