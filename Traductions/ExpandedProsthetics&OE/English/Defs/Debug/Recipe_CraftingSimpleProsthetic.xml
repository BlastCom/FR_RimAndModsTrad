	<RecipeDef ParentName="RecipeSimpleProsthesis">
		<defName>CreateSimpleProstheticLeg</defName>
		<label>Craft simple prosthetic leg</label>
		<description>Crafts a simple prosthetic leg from steel, plasteel and components.</description>
		<descriptionHyperlinks><ThingDef>SimpleProstheticLeg</ThingDef></descriptionHyperlinks>
		<products>
			<SimpleProstheticLeg>1</SimpleProstheticLeg>
		</products>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>55</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
				<li>Plasteel</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>

