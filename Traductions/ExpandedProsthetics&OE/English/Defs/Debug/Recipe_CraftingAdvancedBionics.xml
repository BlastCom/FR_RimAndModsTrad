	<RecipeDef ParentName="RecipeAdvancedProsthesis">
		<defName>CreateExoskeletonSuit</defName>
		<label>Craft exoskeleton suit</label>
		<description>Crafts an exoskeleton suit from steel and plasteel. Requires the 3D printer and a moderate amount of time.</description>
		<descriptionHyperlinks><ThingDef>ExoskeletonSuit</ThingDef></descriptionHyperlinks>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>75</count> 
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>60</count> 
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>5</count> 
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>Steel</li>
				<li>ComponentSpacer</li>
			</thingDefs>
