<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<RecipeDef Abstract="True" Name="RecipeAdvancedProsthesis">
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<soundWorking>EPOEAssembling</soundWorking>
		<unfinishedThingDef>UnfinishedProsthesis</unfinishedThingDef>
		<workSkill>Crafting</workSkill>
		<workAmount>10000</workAmount>
		<jobString>Upgrading bionic.</jobString>
		<skillRequirements>
			<Crafting>10</Crafting>
		</skillRequirements>
		<recipeUsers>
			<li>FabricationBench</li>
		</recipeUsers>
		<factionPrerequisiteTags>
			<li>Ancients</li>  <!-- hides the recipes -->
		</factionPrerequisiteTags>
	</RecipeDef>

	<RecipeDef ParentName="RecipeAdvancedProsthesis">
		<defName>CreateAdvancedBionicEye</defName>
		<label>Upgrade bionic eye</label>
		<description>Upgrades a bionic eye with advanced components.</description>
		<descriptionHyperlinks><ThingDef>AdvancedBionicEye</ThingDef></descriptionHyperlinks>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicEye</li>
				<li>ComponentSpacer</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AdvancedBionicEye>1</AdvancedBionicEye>
		</products>
	</RecipeDef>
	
