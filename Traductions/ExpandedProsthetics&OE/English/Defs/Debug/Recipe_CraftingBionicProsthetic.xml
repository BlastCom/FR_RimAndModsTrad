	<RecipeDef ParentName="RecipeBionicProsthesis">
		<defName>CreateBionicEye</defName>
		<label>Craft bionic eye</label>
		<description>Crafts a bionic eye from plasteel.</description>
		<descriptionHyperlinks><ThingDef>BionicEye</ThingDef></descriptionHyperlinks>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>LightReceptor</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>45</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>LightReceptor</li>
				<li>Plasteel</li>
				<li>ComponentIndustrial</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<BionicEye>1</BionicEye>
		</products>
	</RecipeDef>
	
